---
import Nav from '@components/NavBar';
import BaseHtml from './BaseHtml.astro';
import Footer from '@components/Footer';

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseHtml title={title}>
  <Nav client:load pathname={Astro.url.pathname} />
  <slot />
  <Footer client:load />
</BaseHtml>
