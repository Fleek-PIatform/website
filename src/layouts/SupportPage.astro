---
import '@styles/supportPage.css';

import BaseHtml from './BaseHtml.astro';

interface Props {
  title: string;
}

const { title } = Astro.props;
const description = '';
const image = '';
const slug = '';
---

<BaseHtml
  title={title}
  ogMeta={{
    title,
    description,
    image,
    slug,
  }}
>
  <slot />
</BaseHtml>
