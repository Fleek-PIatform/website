---
import '@styles/supportPage.css';

import BaseHtml from './BaseHtml.astro';
import NavBar from '@components/Support/NavBar';
import Footer from '@components/Support/Footer';

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseHtml title={title}>
  <NavBar client:load />
  <slot />
  <Footer client:load />
</BaseHtml>
