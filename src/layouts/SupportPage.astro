---
import '@styles/supportPage.css';

import BaseHtml from './BaseHtml.astro';

interface Props {
  title: string;
}

const { title } = Astro.props;
const description = '';
const image = '';
const slug = '';
---

<BaseHtml
  title={title}
  ogMeta={{
    title,
    description,
    image,
    slug,
  }}
>
  <slot />

  <script
    id="ze-snippet"
    defer
    src="https://static.zdassets.com/ekr/snippet.js?key=c0ecb1c1-369a-4543-b528-cbba3f2fb35d"
  ></script>
</BaseHtml>
